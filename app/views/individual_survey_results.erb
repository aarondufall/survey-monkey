<% @survey.each do |survey| %>
<p>Title: <%= survey.title %></p>
  <% survey.questions.each do |question|
    <div class="response_count">
      <p><a href="/individual_survey_results/<%= survey.id %>"><%= question.name %></a> | <%= question.response.count %></p>
     </div>
  <% end %>
<% end %>

